<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafx_project.controllers.EditOwnerController">
    <Label text="Edit Owner" />
    <TextField fx:id="idTextField" promptText="Owner ID" />
    <TextField fx:id="nameTextField" promptText="Name" />
    <TextField fx:id="cinTextField" promptText="CIN" />
    <TextField fx:id="addressTextField" promptText="Address" />
    <TextField fx:id="phoneNumberTextField" promptText="Phone Number" />
    <Button text="Update" onAction="#saveChanges" />
</VBox>
